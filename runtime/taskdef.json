{
  "containerDefinitions": [
    {
      "name": "Main",
      "image": "209483893123.dkr.ecr.ap-south-1.amazonaws.com/galani-enterprise:v3",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "command": [
        "sh",
        "-c",
        "echo 'STARTING DEBUG MODE'; python -m http.server 8000 & sleep 3600"
      ],
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/aws/ecs/default/galani-enterprise-9f3e",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "family": "default-galani-enterprise",
  "taskRoleArn": "arn:aws:iam::209483893123:role/galani-task-role",
  "executionRoleArn": "arn:aws:iam::209483893123:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}
