{
  "containerDefinitions": [
    {
      "name": "Main",
      "image": "209483893123.dkr.ecr.ap-south-1.amazonaws.com/galani-enterprise:a7186be",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "command": [
        "uvicorn",
        "galani.api.app_v3:app",
        "--host",
        "0.0.0.0",
        "--port",
        "8000"
      ],
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/aws/ecs/galani-api",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "family": "default-galani-enterprise",
  "taskRoleArn": "arn:aws:iam::209483893123:role/galani-task-role",
  "executionRoleArn": "arn:aws:iam::209483893123:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}
